{
  "name": "task-calendar-server",
  "version": "1.0.0",
  "description": "Task Calendar challenge back-end",
  "main": "./api/server.js",
  "scripts": {
    "prestart": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all $",
    "start": "nodemon ./api/server.js",
    "drop": "npx sequelize-cli db:drop $",
    "seed": "npx sequelize-cli db:seed:all $",
    "unseed": "npx sequelize-cli db:seed:undo $",
    "test": "nyc --all --include models --include controllers mocha test/**/*.js --exit"
  },
  "author": "Joao Marco Matta",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.4.1",
    "sinon": "^13.0.1"
  }
}
